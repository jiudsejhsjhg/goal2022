将存储于array中
function arrayToTree(array ,rootPos = 0) {
  if (array[rootPos] == null) {
    return null
  }
  var root = {
    val: array[rootPos],
    left: null,
    right: null,

  }

  root.left = arrayToTree[array,root * 2 + 1]
  root.right = arrayToTree(array[rootPos * 2 + 2])

  return root


}



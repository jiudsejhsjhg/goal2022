function reduce(ary,reduce ,initialValue) {
  var result = initialValue
  var start = 0
  if (initialValue == undefined) {
    result = ary[0]
    start = 1
  }
  for (var i = start ; i < ary.length; i++) {
    result = reduce(result,ary[i], i ,ary)

  }
  return result
}
